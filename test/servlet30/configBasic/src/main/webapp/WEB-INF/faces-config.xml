<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0"
              metadata-complete="true">

    <component>
        <component-type>DefaultComponent</component-type>
        <component-class>com.sun.faces.test.servlet30.configBasic.TestComponent</component-class>
    </component>

    <converter>
        <converter-id>DefaultConverter</converter-id>
        <converter-class>com.sun.faces.test.servlet30.configBasic.TestConverter</converter-class>
    </converter>

    <validator>
        <validator-id>DefaultValidator</validator-id>
        <validator-class>com.sun.faces.test.servlet30.configBasic.TestValidator</validator-class>
    </validator>

    <render-kit>
        <renderer>
            <component-family>Test</component-family>
            <renderer-type>DefaultRenderer</renderer-type>
            <renderer-class>com.sun.faces.test.servlet30.configBasic.TestRenderer</renderer-class>
        </renderer>
    </render-kit>
  
</faces-config>
